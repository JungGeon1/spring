<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper 
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.test.nb.dao.nbAdminMemberDao">


	
	<!-- 관리자아이디확인 -->
		<select id="selectAdminIdChk" resultType="com.test.nb.domain.NbAdminMemberDto">
			select * from noticetest.noticeadminmember where admin_id=#{param};
		</select>
		
	<!-- 관리자 목록 출력 -->
		<select id="adminLsit" resultType="com.test.nb.domain.NbAdminMemberDto">
			select * from noticetest.noticeadminmember;
		</select>	
	<!-- 회ㅝㄴ 목록 출력 -->
		<select id="memberLsit" resultType="com.test.nb.domain.NbMemberDto">
			select * from noticetest.noticeBoardMember;
		</select>
		
		<delete id="memberDelete">
			delete from noticetest.noticeBoardMember where nbm_idx=#{param}; 
		</delete>
			
</mapper>
